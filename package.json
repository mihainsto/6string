{
  "private": true,
  "name": "6string",
  "workspaces": [
    "client",
    "api"
  ],
  "scripts": {
    "start:client": "yarn workspace client run start",
    "start:api": "yarn workspace api run start:dev",
    "docker:db": "docker-compose up postgres",
    "docker:reset": "docker exec -it 6string_api yarn workspace api run prisma:reset",
    "docker:prod": "docker-compose up"
  },
  "devDependencies": {
    "typescript": "^4.1.3"
  }
}
